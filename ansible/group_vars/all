# apache2
virtualhosts:
  - name: vhost1
    datos:
      nameserver: www.pagina1.org
      documentroot: /srv/www/pagina1
      errorlog: error_example1
      accesslog: access_example1
  - name: vhost2
    datos:
      nameserver: www.pagina2.org
      documentroot: /var/www/pagina2
      errorlog: error_example2
      accesslog: access_example2
rmvhost:
  - name: pagina2

